public class P49 {
	public static void main(String[] args) {
		int [][]num;
		num = new int[2][5];
		num[0][0] = 100;
		num[0][1] = 88;
		num[0][2] = 90;
		num[0][3] = 90;
		num[0][4] = 95;
		num[1][0] = 95;
		num[1][1] = 86;
		num[1][2] = 40;
		num[1][3] = 23;
		num[1][4] = 1;
		for (int i = 0; i<=num.length-1; i++) {
			System.out.println(" 第"+ (i+1) +"人 數學分數: " + num[0][i]);
			System.out.println(" 第"+ (i+1) +"人 國文分數: " + num[1][i]);
		}
	}
}
